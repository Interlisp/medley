                       (PUTBASE VA 0 (READOCT))))
              (I [ALLOCAL (COND
                             [(NULL (GETD 'INSPECT]
                             ((RECLOOK 'IFPAGE)
                              (INSPECT [COND
                                          ((LISTP VMEMFILE)
                                           (VMAPPAGE (fetch (POINTER PAGE#) of \InterfacePage
                                                            )))
