(PUTPROPS \CCODENARGS MACRO ((FNH)
                                     ([LAMBDA (N)
                                        (COND
                                           ((ILESSP N 0)
                                            1)
                                           (T N]
                                      (fetch (FNHEADER NA) of FNH))))

(PUTPROPS \CCODEFNTYP MACRO ((FNH)
                                     (SELECTQ (\CCODEARGTYPE FNH)
                                         (0 'CEXPR)
                                         (1 'CFEXPR)
                                         (2 'CEXPR*)
                                         'CFEXPR*)))

