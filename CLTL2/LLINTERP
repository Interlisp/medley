                  [(NLISTP (CDR *TAIL*))
                   (RETURN (\EVAL (CAR *TAIL*]
                  (T (\EVAL (CAR *TAIL*))
                     (SETQ *TAIL* (CDR *TAIL*))
                     (GO LP])

(COND
  [NLAMBDA U
    (DECLARE (SPECVARS *TAIL*))                          (* lmm "25-APR-80 18:03")
