;; AR 6247 test;; Filed as {ERIS}<LISPCORE>TEST>CMLSTREAMS>AR6247.TEST;; Verify that WITH-OUTPUT-TO-STRING and WITH-INPUT-FROM-STRING can handle 16-bit characters.(do-test-group AR6427	:before	(test-setq fatstring (il:mkstring (il:packc '(9865 9866 9988))))	(do-test AR6247		(with-input-from-string (s fatstring :index j)(read s))		(with-input-from-string (s fatstring :index k :start 1)(read s))		(mapcar #'(lambda (stringlen) (= 3 stringlen))			(list j k				(LENGTH					(WITH-OUTPUT-TO-STRING						(STREAM (MAKE-ARRAY 10 :ELEMENT-TYPE 'STRING-CHAR :FILL-POINTER 0))						(PRINT FATSTRING STREAM)					)				)			)		)	))