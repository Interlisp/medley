;; AR 7993 test;; Filed as {ERIS}<LISPCORE>TEST>CMLSTRING>AR7993.TEST;; By Peter Reidy;; Verify that CLtL's string comparison functions work on single characters as well as strings.(do-test AR7993	(and		(string= #\a #\a)		(string-equal #\a #\A)		(string< #\a #\b)		(string> #\8 #\5)		(string<= #\a #\b)		(string<= #\b #\b)		(string>= #\8 #\5)		(string>= #\linefeed #\linefeed)		(string/= #\a #\A)		(string-lessp #\a #\B)		(string-not-lessp #\B #\a)		(string-greaterp #\B #\a)		(string-not-greaterp #\a #\B)		(string-not-equal #\a #\B)	))