;; Function To Be Tested: ACONS;;;; Source: Guy L Steele's CLTL;; Section: 15.6 Association Lists;; Page: 279;;;; Created By:    Kelly Roach;;;; Creation Date: June 27,1986;;;; Last Update:   June 27,1986;;;; Filed As:      {ERIS}<LISPCORE>CML>TEST>15-6-ACONS.TEST;;;;;; Syntax:        (ACONS KEY DATUM ALIST);;;; Function Description:;; ACONS constructs a new association list by adding the pair;; (KEY . DATUM) to the old A-LIST.;; ;;     (ACONS X Y A) = (CONS (CONS X Y) A);;     ;; ;; Argument(s):   KEY - a function;;                DATUM - anything;;                ALIST - an association list;;;; Returns:       an association list;;(DO-TEST "ACONS TEST 1" (EQUAL (ACONS 'G 5 '((B . 7)                                             (E . 5)                                             (E . 2)))                               '((G . 5)                                 (B . 7)                                 (E . 5)                                 (E . 2)))       (EQUAL (ACONS 'C 5 '((G . 3)))              '((C . 5)                (G . 3)))       (EQUAL (ACONS 'G 9 '((G . 4)                            (B . 5)                            (E . 7)                            (X . 4)                            (B . 10)                            (X . 6)))              '((G . 9)                (G . 4)                (B . 5)                (E . 7)                (X . 4)                (B . 10)                (X . 6)))       (EQUAL (ACONS 'F 3 '((F . 4)                            (Y . 2)))              '((F . 3)                (F . 4)                (Y . 2)))       (EQUAL (ACONS 'D 3 '((D . 5)                            (Z . 3)                            (Y . 3)                            (Z . 6)))              '((D . 3)                (D . 5)                (Z . 3)                (Y . 3)                (Z . 6)))       (EQUAL (ACONS 'Z 1 '((E . 4)                            (F . 2)                            (G . 3)