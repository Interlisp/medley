;; Regression tests for the SETF machinery(do-test "AR 7507: SETF macroexpands too early sometimes"   (progn      (with-open-file (s "{Core}AR7507.lisp" :direction :output)         (princ ";;                 (defmacro foo (x) `(bar ,x))                 (defsetf foo set-foo)                 (defun baz (y) (setf (foo y) 17))                 " s))      (compile-file "{Core}AR7507.lisp")))