;; Regression tests for the file CMLSPECIALFORMS(do-test "AR 7464: SELECTQ's optimizer should do constant-folding when selector is (SYSTEMTYPE)"   (equal (macroexpand-1 '(il:selectq (il:systemtype)                              (il:interlisp-10 (garbage))                              ((il:tenex il:tops-20) (il:more-garbage))                              ((il:d il:maxc) (il:wonderfulness) (il:brilliance))                              (il:darn)))          '(progn (il:wonderfulness) (il:brilliance))))