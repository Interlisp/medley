(do-test "format-T-regression"	(string= (format nil "~2Tfoo") "  foo"))(do-test "format-E-regression"	(string= (FORMAT NIL "~13,6,2,7E" 3.14159) " 3141590.E-06"))(do-test "format-*-regression"	(string= (FORMAT NIL "~D ~D ~@*~D" 4 5 6) "4 5 4"))(do-test "format-R-regression"	(string= (format nil "~,20,'*R" 4) "****************four"))(do-test "format-:@-@:-regression"	(string= (format nil "~:@R" 42)(format nil "~@:R" 42)))