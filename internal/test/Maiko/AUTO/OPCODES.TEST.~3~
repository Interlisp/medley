(DEFTEST (UNWIND :COMPILED) (UNWINDTESTER))

(DEFTEST (UNWIND-2 :COMPILED) (FOR I FROM 0 TO 100 ALWAYS (EQ (QUOTE SUCCESS) (UW2.TEST I))))



