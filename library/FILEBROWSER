                  ITEM)))                 (|add| (|fetch| (FILEBROWSER DELETEDPAGES) |of| FBROWSER)                        SIZE)))             T))))))(DEFINEQ(FB.UNDELETECOMMAND  (LAMBDA (BROWSER)                                          (* |bvm:| "12-Sep-85 15:44")    (TB.MAP.SELECTED.ITEMS (|fetch| (FILEBROWSER TABLEBROWSER) |of| BROWSER)           (FUNCTION FB.UNDELETE.FILE))    (FB.UPDATE.COUNTERS BROWSER)))(FB.UNDELETEALLCOMMAND  (LAMBDA (BROWSER)                                          (* |bvm:| "18-Sep-85 12:20")    (TB.MAP.ITEMS (|fetch| (FILEBROWSER TABLEBROWSER) |of| BROWSER)           (FUNCTION FB.UNDELETE.FILE))    (FB.UPDATE.COUNTERS BROWSER)))(FB.UNDELETE.FILE  (LAMBDA (TBROWSER ITEM)                                    (* |bvm:| "13-Oct-85 17:44")    (COND       ((|fetch| TIDELETED |of| ITEM)        (LET ((FBROWSER (TB.USERDATA TBROWSER))              SIZE)             (TB.UNDELETE.ITEM TBROWSER ITEM)             (|add| (|fetch| (FILEBROWSER DELETEDFILES) |of| FBROWSER)                    -1)             (COND                ((SETQ SIZE (|fetch| (FBFILEDATA SIZE) |of| (|fetch| TIDATA |of|                                                                                        ITEM)))                 (|add| (|fetch| (FILEBROWSER DELETEDPAGES) |of| FBROWSER)                        (IMINUS SIZE))))))))))(DEFINEQ(FB.COPYCOMMAND  (LAMBDA (BROWSER)                                          (* \;                                                            "Edited 19-Oct-90 17:44 by gadener")    (FB.COPY/RENAME.COMMAND BROWSER '|Copy| (CONS (FUNCTION COPYFILE)))))(FB.RENAMECOMMAND  (LAMBDA (BROWSER)                                          (* \;                                                            "Edited 19-Oct-90 18:57 by gadener")    (FB.COPY/RENAME.COMMAN