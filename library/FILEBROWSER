       (FILEBROWSER                                                                                  INFOMENUCHOICES)                                                                                   |of| BROWSER))                                                                 W PROTO                      |when| (MEMB (|fetch| (INFOFIELD INFONAME) |of| SPEC)                                       WANTED)                      |collect| (SETQ SPEC (COPY SPEC))                            (|if| (SETQ PROTO (|fetch| (INFOFIELD INFOPROTOTYPE) |of|                                                                                         SPEC))                                |then|                   (* \;                                   "Have prototypical example, use it to get better width estimate.")                                      (SETQ W (STRINGWIDTH PROTO FONT))                                      (|replace| (INFOFIELD INFOWIDTH) |of| SPEC                                         |with| (+ W (TIMES 2 (CHARWIDTH (CHARCODE X)                                                                         FONT))))                                      (|if| (LISTP (|fetch| (INFOFIELD INFOFORMAT)                                                          |of| SPEC))                                          |then| (RPLACA (CDR (|fetch| (INFOFIELD INFOFORMAT)                                                                     |of| SPEC))                                                            W)))                            SPEC)))               (FB.SET.BROWSER.TITLE BROWSER)               (CLEARW (|fetch| (FILEBROWSER COUNTERWINDOW) |of| BROWSER))               (CLEARW (|fetch| (FILEBROWSER HEADINGWINDOW) |of| BROWSER))                                                             (* \;                                                 "Cle