RSOREXIT  [LAMBDA NIL                                          (* ; "Edited 11-Aug-89 13:16 by takeshi")         (* * called when cursor moves off the screen edge)    (DECLARE (GLOBALVARS LASTSCREEN LASTMOUSEX LASTMOUSEY \MAIKO.CURRENT.SCREEN.MODE))    (PROG (SCREEN XCOORD YCOORD SCREEN2 XCOORD2 YCOORD2)          (SETQ SCREEN LASTSCREEN)          (SETQ XCOORD LASTMOUSEX)          (SETQ YCOORD LASTMOUSEY)          [SETQ SCREEN2 (COND                           ((EQ SCREEN \MAINSCREEN)                            (PROGN \COLORSCREEN))                           (T (PROGN \MAINSCREEN]            (* generalize for more than two                                                            screens (or alternate physical                                                            arrangement of screens.))          (COND             ((EQ XCOORD 0)              (SETQ XCOORD2 (IDIFFERENCE (fetch (SCREEN SCWIDTH) of SCREEN2)                                   2)))             ((EQ XCOORD (SUB1 (fetch (SCREEN SCWIDTH) of SCREEN)))              (SETQ XCOORD2 1))             (T (RETURN)))          [SETQ YCOORD2 (IQUOTIENT (ITIMES YCOORD (SUB1 (fetch (SCREEN SCHEIGHT) of SCREEN2))                                          )                               (SUB1 (fetch (SCREEN SCHEIGHT) of SCREEN]          (CURSORSCREEN SCREEN2 XCOORD2 YCOORD2])(CURSORSCREEN  [LAMBDA (SCREEN XCOORD YCOORD)                       (* ; "Edited 19-Jun-90 16:33 by matsuda")         (* * sets up SCREEN to be the current screen, XCOORD %, YCOORD is initial pos        of cursor on SCREEN)    (COND       ((NULL XCOORD)        (SETQ XCOORD 0)))    (COND       ((NULL YCOORD)        (SETQ YCOORD 0)))    (PROG (DESTINATION)          (SETQ DESTINATION (fetch (SCREEN SCDESTINATION) of SCREEN))          (\CURSORDOWN)          (SETQ \CURSORSCREEN SCREEN)          (\CURSORDESTINATION DESTIN