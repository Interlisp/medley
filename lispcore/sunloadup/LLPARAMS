[PUTPROPS EMADDRESS MACRO (ARGS ([LAMBDA (ADDR)
                                   (COND
                                      [(EQ \D1BCPLspace \D0BCPLspace)
                                       (LIST (BIG-VMEM-CODE (LIST 'OPCODES 'GCONST 0 0
                                                                  (LRSH ADDR 10Q)
                                                                  (LOGAND ADDR 377Q))
                                                    (LIST 'OPCODES 'GCONST 0 (LRSH ADDR 10Q)
                                                          (LOGAND ADDR 377Q]
                                      (T `(\VAG2 (fetch EmulatorSpace of \InterfacePage)
                                                 %, ADDR]
                                 (EVAL (CAR ARGS]

(PUTPROPS EMGETBASE MACRO ((OFFSET)
                           (\GETBASE (EMADDRESS OFFSET)
                                  0)))

(PUTPROPS EMPUTBASE MACRO ((OFFSET VAL)
                           (\PUTBASE (EMADDRESS OFFSET)
                                  0 VAL)))

