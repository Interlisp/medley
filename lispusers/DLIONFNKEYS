           do (TEDIT.INSERT.OBJECT (KEYOBJ.CREATE KEY LABEL T)                       TXT))                                 (* ;    "this will create abortable key objects (if you slide out of the region, no transitions are sent)")          (TEDIT.SETSEL TXT 2 0 'LEFT)          (TEDIT.PARALOOKS TXT '(QUAD CENTERED))             (* ;                                      "(TEDIT.NORMALIZECARET TXT (TEDIT.SETSEL TXT 0 0 (QUOTE LEFT)))")          (SETQ W (CREATEW (CREATEREGION (IQUOTIENT (IDIFFERENCE (BITMAPWIDTH (SCREENBITMAP))                                                           WIDTH)                                                2)                                  5 WIDTH (IPLUS (FONTPROP MENUFONT 'HEIGHT)                                                 (BITMAPHEIGHT KEYOBJ.TEMPLATE)                                                 10))                         "Dandelion function keys" 2))          (SCROLLW W 0 -5)                                   (* ;                                                              "used to have NOTITLE T in the props")                                                             (* ;                                             "TEDIT TXT W NIL (QUOTE (LEAVETTY T PROMPTWINDOW DON'T))")          (OPENTEXTSTREAM TXT W NIL NIL '(READONLY T))          (WINDOWPROP W 'WINDOWENTRYFN 'NIL)                 (* ;                                                            "(WINDOWPROP W (QUOTE TITLE) (QUOTE NIL))")          (WINDOWPROP W 'ICON FNKEYICON)          (WINDOWPROP W 'RIGHTBUTTONFN 'FNKEY.MENUFN)          (SETQ DLIONFNKEYS W])(FNKEY.MENUFN  [LAMBDA (KEYWINDOW)                                        (* gbn "28-Jan-85 01:17")    (PROG ((ITEM (MENU FNKEY.MENU)))          (COND             (ITEM (APPLY* ITEM KEYWINDOW]))(RPAQ? FNKEY.MENU [create MENU ITEMS _ '((Close 'CLOSEW "Closes a window")                                         (Bury 'BUR