          (LET (LINE POSLINK POSFRAG POSENDLINK POSENDTERM POSTERM LINK)
               (while (SETQ LINE (CL:READ-LINE STREAM NIL))
                  when [AND (SETQ POSLINK (STRPOS "<LI><A REL=DEFINITION HREF=%"../Body/" LINE 1 NIL
                                                 NIL T))
