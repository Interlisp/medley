(ISIGN
  [LAMBDA (VALUE SIGN)                                   (* MD " 4-JAN-84 16:55")
    (COND
       ((IGREATERP SIGN 0)
        VALUE)
       ((ILESSP SIGN 0)
        (ITIMES (IABS VALUE)
               -1))
       (T                                                    (* If sign is 0 then make + or -
                                                           randomly)
          (ITIMES (IABS VALUE)
                 (CAR (NTH '(-1 1)
                           (RAND 1 2])

(JOYSTICK
  [LAMBDA NIL                                            (* MD "28-NOV-83 12:13")

         (* Pinouts%: (Atari D0) (Fire PAD1) (E PAD2)
       (W PAD3) (S PAD4) (N PAD5) Atari connector%: Top row
       (view from outside) 0 E W S N; bottom row%: 0 GND 0 Fire ;
       D0 connector%: middle row%: 0 0 Fire E W S N;
       bottom row GND 0 0 0 0 0)

    (COND
       ((KEYDOWNP 'PAD2)
        4)
       ((KEYDOWNP 'PAD3)
        8)
       ((KEYDOWNP 'PAD4)
        1)
       ((KEYDOWNP 'PAD5)
        2])

(LOSE
