                                           11)
                              6)
           (IDIFFERENCE (ITIMES (IDIFFERENCE 30 (GETPROP PMAN 'YLOC))
                               11)
                  6)
           16 16 PSTREAM)
    (COND
       ((EQ NUMMEN 2)
        (ERASEOBJ PMAN 40 345 16 16 PSTREAM))
       ((EQ NUMMEN 1)
        (ERASEOBJ PMAN 20 345 16 16 PSTREAM))
       ((EQ NUMMEN 0)
        (SHOWCAP "GAME OVER" (ITIMES 11 11)
               (ITIMES (IDIFFERENCE 30 17)
                      11))
