(FILESLOAD (SOURCE) TCPEXPORTS)
)
(DECLARE%: DONTEVAL@LOAD DOCOPY 

(CHANGENAME (QUOTE UDP.SEND) (QUOTE \UDP.SET.CHECKSUM) (QUOTE \UDP.SET.CHECKSUM.ZERO))
)
