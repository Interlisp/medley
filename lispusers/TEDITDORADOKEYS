        ("1,+" FN \\TEDIT.STRIKEOUT.SEL.OFF)
        ("1,-" FN \\TEDIT.UNDERLINE.SEL.ON)
        ("1,_" FN \\TEDIT.UNDERLINE.SEL.OFF)
