          (if (OPENP STREAM)
              then (PRINTOUT STREAM .OP (CHARCODE ^U)
                              |.P2|
                              (LIST NEWLOOKS (TALK.ENCODE.SEL CH#ORSEL LEN))
                              .FORCE])

(TALK.PARAFN
  [LAMBDA (TEXTOBJ NEWLOOKS CH#ORSEL LEN)                (* ; "Edited  9-Jun-88 15:10 by cdl")
