(ADDTOVAR PRINTOUTMACROS [.OP (LAMBDA (COMS)
                                         (CONS `(PROGN (PRINTCCODE (CHARCODE BS)
                                                              NIL)
                                                       (PRINTCCODE ,(CADR COMS)
                                                              NIL))
                                               (CDDR COMS]
                             [.FORCE (LAMBDA (COMS)
                                            (CONS '(FORCEOUTPUT NIL)
                                                  (CDR COMS])
