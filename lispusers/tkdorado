                                                        'SLOPE                                                         (CADADR (RECORDACCESSFORM 'CLITAL))                                                         'WEIGHT                                                         (CADADR (RECORDACCESSFORM 'CLBOLD]                                       (CAR LOOKS))                         charlooks                         (FMEMB (CADR LOOKS)                                '(ITALIC BOLD ON]                  (if (OR (AND (NEQ (CAR LOOKS)                                    'SLOPE)                               (NEQ (CAR LOOKS)                                    'WEIGHT))                          (\TK.SETFONTINLOOKS TEXTSTREAM charlooks))                      then (TEDIT.CARETLOOKS TEXTSTREAM charlooks)                           (if TEDITKEY.VERBOSE                               then (\TK.SHOWCARETLOOKS TEXTSTREAM TEXTOBJ SEL]      else (TEDIT.LOOKS TEXTSTREAM LOOKS SEL]))[SETQ TEDITKEY.KEYBINDINGS (UNION (APPEND TEDITKEY.KEYBINDINGS '(NIL))                                  '(([LAMBDA (TEXTSTREAM TEXTOBJ SEL)                                            (if (ZEROP (fetch DCH of SEL))                                                then                                                (\TK.DEFAULTS.CARET TEXTSTREAM TEXTOBJ SEL)                                                else                                                (\TK.DEFAULTSSEL TEXTSTREAM TEXTOBJ SEL]                                     (%##^V)                                     "default looks")                                    ([LAMBDA (TEXTSTREAM TEXTOBJ SEL)                                            (\TKD.SETLOOKS TEXTSTREAM TEXTOBJ SEL '(OVERLINE ON]                                     (%##^D)                                     "overbar on")                                    ([LAMBDA (TEXTSTREAM TEXTOBJ SEL)                                            (\TKD.S