                T)
         (CL:WHEN (MOUSESTATE MIDDLE)
             (SELECTQ (MENU (|create| MENU
                                   TITLE _ (CONCAT XREF.TAG " Menu")
                                   ITEMS _ '(|Find Definition| |Change Reference| |Change Display|)
                                   CENTERFLG _ T))
                 (|Find Definition| 
                      (LET ((CHNOS (TSP.LIST.OF.OBJECTS TSTREAM (FUNCTION (LAMBDA (OBJ CHNO TAG)
                                                                            (AND
                                                                             (NUMBEROBJP OBJ)
                                                                             (EQ TAG
                                                                                 (IMAGEOBJPROP
                                                                                  OBJ
                                                                                  'TAG)))))
                                          XREF.TAG
                                          'CH#)))
