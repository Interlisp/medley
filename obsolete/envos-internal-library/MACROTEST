									    VAR))
							       (CDR X)))))
			     (for X on (CDR D) do (MTCHECK (EQ (\REFCNT X)
							       1)
							   (EQ (\REFCNT (CAR X))
							       1)))
			     (for I from 0 to 1000Q do (MTCHECK (EQUAL (pop D)
								       (LIST I))))
			     (for X
				in (QUOTE ((((A) . B))
					    (A B)
					    ((A B . C))
					    ((A B C D . E))
					    ((A . B)
					     (C . D)
					     (E . F)
					     ((((((((((((((G)))))))))))))))
					    (1 2 3 4 5 6 7 10Q 11Q 12Q 13Q 14Q 15Q 16Q 17Q 20Q 21Q 22Q
