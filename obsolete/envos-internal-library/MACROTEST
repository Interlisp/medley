									   AY)
								       (EQ (CAR Y)
									   AX))))
					          (MTCHECK (PROGN (swap (CDR Y)
									(CDR X))
								  (AND (EQ (CDR X)
									   DY)
								       (EQ (CDR Y)
									   Y))))
					          (MTCHECK (PROGN 
                                                             (* Now put them back)
								  (swap (CAR X)
									(CAR Y))
								  (swap (CDR Y)
									(CDR X))
