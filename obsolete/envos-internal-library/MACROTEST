									 YY))))))))))))

(!COPY
  (LAMBDA (X)                                                (* lmm "25-FEB-81 21:47")
    (COND
      ((NLISTP X)
	X)
      (T (CONS (!COPY (CAR X))
	       (!COPY (CDR X)))))))

(!SMASH
  (LAMBDA (X)                                                (* bvm: "14-JAN-82 17:04")
    (COND
      ((LISTP X)
	(RPLACD (RPLACA X (!SMASH (CAR X)))
		(!SMASH (CDR X))))
      ((NULL X)
	(RPLACA (RPLACD X X)
		X))
      (T X))))

