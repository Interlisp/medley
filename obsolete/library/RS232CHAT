TREAM)          (SETQ STREAM (OPENSTREAM '{RS232} 'INPUT))          (SETQ OUTSTREAM (RS232C.OUTPUTSTREAM))          (STREAMPROP STREAM 'SETDISPLAYTYPE (FUNCTION NILL))          (STREAMPROP STREAM 'READPEVENT (RS232C.READP.EVENT STREAM))          (STREAMPROP STREAM 'LOGINFO (FUNCTION NILL))          (STREAMPROP STREAM 'FLUSH&WAIT (FUNCTION NILL))          (STREAMPROP STREAM 'SENDSCREENPARAMS (FUNCTION NILL))          [STREAMPROP STREAM 'OPTIONS '(("Set Line Parameters" 'RS232CHAT.SET.PARAMETERS                                                "Modify RS232 parameters"]          (RETURN (CONS STREAM OUTSTREAM])(RS232CHAT.SET.PARAMETERS  (LAMBDA NIL                                                (* ejs%: "24-Dec-85 14:57")    (LET* ((WINDOW WINDOW)           (PWINDOW (GETPROMPTWINDOW WINDOW)))          (COND             ((FNTYP 'RS232CMENU.SET.PARAMETERS)              (ALLOW.BUTTON.EVENTS)              (RESETFORM (CURSOR WAITINGCURSOR)                     (RS232CMENU.SET.PARAMETERS WINDOW))              (printout PWINDOW T "Select desired parameters.  Choose Apply! when done."))             (T (printout PWINDOW T "You must load RS232CMENU to use this function")))))))(ADDTOVAR NETWORKOSTYPES (RS232))(ADDTOVAR CHAT.PROTOCOLTYPES (RS232 . RS232CHAT.HOST.FILTER))(FILESLOAD DLRS232C CHAT)(PUTPROPS RS232CHAT COPYRIGHT ("Venue & Xerox Corporation" 1985 1986 1987 1990))(DECLARE%: DONTCOPY  (FILEMAP (NIL (992 3215 (RS232CHAT.HOST.FILTER 1002 . 1725) (RS232CHAT.OPEN 1727 . 2624) (RS232CHAT.SET.PARAMETERS 2626 . 3213)))))STOP