bind ITEM VALUE for ID in '(BaudRate Parity CharLength FlowControl StopBits)                           collect (SETQ ITEM (FM.GETITEM ID NIL WINDOW))                                 (SETQ VALUE (FM.ITEMPROP ITEM 'STATE))                                 (SELECTQ ID                                     (BaudRate (CONS 'LINE.SPEED VALUE))                                     (Parity (CONS 'PARITY (U-CASE VALUE)))                                     (CharLength (CONS 'CHAR.LENGTH (SELECTQ VALUE                                                                        (Five 5)                                                                        (Six 6)                                                                        (Seven 7)                                                                        (Eight 8)                                                                        (ERROR                                                                         "Impossible character length"                                                                                VALUE))))                                     (FlowControl (CONS 'FLOW.CONTROL (SELECTQ VALUE                                                                          (XOnXOff 'XOnXOff)                                                                          (None NIL)                                                                          (ERROR                                                                             "Impossible flow control"                                                                                  VALUE))))                                     (StopBits (CONS 'STOP.BITS (SELECTQ VALUE                                                                    (One 1)                                                                    (Two 2)                                                                    (ERROR "Impossible stop bits"                                                                            VALUE))))                                     NIL)))          [COND             ((EQ DEVICENAME 'RS232)              (RS232C.REPORT.STATUS (SELECTQ (FM.ITEMPROP (FM.GETITEM 'Report NIL WINDOW)                                                    'STATE)                                        (Always T)                                        (Never NIL)                                        (Output% Only 'OUTPUT)                                        (Input% Only 'INPUT)                                        (ERROR "Impossible reporting status requested"                                               (FM.ITEMPROP (FM.GETITEM 'Report NIL WINDOW)                                                      'STATE]          (printout (GETPROMPTWINDOW (MAINWINDOW WINDOW))                 T                 (SELECTQ DEVICENAME                     (RS232 "RS232 port parameters set.")                     (TTY "TTY port parameters set.")                     "Please select a device before setting parameters."])(RS232C.FMENU.HANGUP  [LAMBDA (ITEM WINDOW BUTTON)                               (* ejs%: " 5-Sep-85 14:06")    (COND       ((MOUSECONFIRM "This will break your connection; please confirm"                "Select the left mouse button to confirm" (GETPROMPTWINDOW (MAINWINDOW WINDOW)))        (RS232MODEMHANGUP])(RS232C.FMENU.SENDBREAK  [LAMBDA (ITEM WINDOW BUTTON)                               (* ejs%: " 5-Sep-85 14:07")    (COND       ((MOUSECONFIRM "This may break your connection; please confirm"                "Select the left mouse button to confirm" (GETPROMPTWINDOW (MAINWINDOW WINDOW)))        (RS232SENDBREAK])(RS232C.FMENU.SENDLONGBREAK  [LAMBDA (ITEM WINDOW BUTTON)                               (* ejs%: " 5-Sep-85 14:07")    (COND       ((MOUSECONFIRM "This may break your connection; please confirm"                "Select the left mouse button to confirm" (GETPROMPTWINDOW (MAINWINDOW WINDOW)))        (RS232SENDBREAK T])(RS232C.FMENU.SHOW  [LAMBDA (ITEM WINDOW BUTTON)                               (* ejs%: " 6-May-86 16:56")    (LET [(DEVICENAME (WINDOWPROP WINDOW 'CHATDEVICE]         (COND            (DEVICENAME (RS232CMENU.FILLIN.PARAMETERS WINDOW DEVICENAME])(RS232CMENU.FILLIN.PARAMETERS  [LAMBDA (WINDOW DEVICENAME)                                (* jow "26-Jun-86 17:44")    (LET [(INFOFN (SELECTQ DEVICENAME                      (TTY (FUNCTION TTY.GET.PARAMETERS))                      (RS232 (FUNCTION RS232C.GET.PARAMETERS))                      (FUNCTION NILL]         (WINDOWPROP WINDOW 'CHATDEVICE DEVICENAME)         (bind ITEM for PROP.VAL in (APPLY* INFOFN '(LINE.SPEED PARITY CHAR.LENGTH FLOW.CONTROL                                                            STOP.BITS)) as ID            in '(BaudRate Parity CharLength FlowControl StopBits)            do (SETQ ITEM (FM.GETITEM ID NIL WINDOW))               (FM.CHANGESTATE ITEM (SELECTQ ID                                        (BaudRate (CDR PROP.VAL))                                        (Parity (SELECTQ (CDR PROP.VAL)                                                    (EVEN 'Even)                                                    (ODD 'Odd)                                                    (NONE 'None)                                                    (ERROR "Impossible parity setting" (CDR PROP.VAL)                                                           )))                                        (CharLength (SELECTQ (CDR PROP.VAL)                                                        (5 'Five)                                                        (6 'Six)                                                        (7 'Seven)                                                        (8 'Eight)                                                        (ERROR "Impossible character length"                                                               (CDR PROP.VAL))))                                        (FlowControl (COND                                                        [(LISTP (CDR PROP.VAL))                                                         (SELECTQ (CADR PROP.VAL)                                                             (0 'None)                                                             (1 'XOnXOff)                                                             (ERROR "Unknown flow control setting"                                                                    (CDR PROP.VAL]                                                        ((EQ (CDR PROP.VAL)                                                             'XOnXOff)                                                         'XOnXOff)                                                        (T 'None)))                                        (StopBits (SELECTQ (CDR PROP.VAL)                                                      (1 'One)                                                      (2 'Two)                                                      (ERROR "Unknown stopbits setting" (CDR PROP.VAL                                                                                             ))))                                        NIL)                      WINDOW))         (COND            ((EQ DEVICENAME 'RS232)             (FM.CHANGESTATE (FM.GETITEM 'Report NIL WINDOW)                    (SELECTQ (RS232C.REPORT.STATUS)                        (T 'Always)                        (OUTPUT 'Output% Only)                        (INPUT 'Input% Only)                        (NIL 'Never)                        (ERROR "Impossible status reporting option" (RS232C.REPORT.STATUS)))                    WINDOW])(RS232CMENU.SET.PARAMETERS  [LAMBDA (MAINWINDOW)                                       (* ; "Edited  5-Nov-87 11:16 by FS")                    (* ;; "Set the RS232 menu settings from the current RS232 line parameters.")    (LET* ((CHATSTATE (WINDOWPROP MAINWINDOW 'CHATSTATE))           (STREAM (fetch (CHAT.STATE INSTREAM) of CHATSTATE))           (DEVICENAME (fetch (FDEV DEVICENAME) of (fetch (STREAM DEVICE) of STREAM)))           (FMENUTITLE NIL)           (FMSPEC (SELECTQ DEVICENAME                       (TTY (SETQ FMENUTITLE "TTY Port Settings")                            (SELECTC \MACHINETYPE                                (\DANDELION DLTTY.FMENU.SPEC)                                (\DAYBREAK DVTTY.FMENU.SPEC)                                (ERROR DEVICENAME " is not supported on this machine")))                       (RS232 (SETQ FMENUTITLE "RS232 Port Settings")                              (SELECTC \MACHINETYPE                                  (\DANDELION DLRS232C.FMENU.SPEC)                                  (\DAYBREAK DVRS232C.FMENU.SPEC)                                  (ERROR DEVICENAME " is not supported on this machine")))                       (ERROR "unknown device" DEVICENAME)))           (FMENU (FREEMENU FMSPEC FMENUTITLE)))          (GETPROMPTWINDOW MAINWINDOW 2)          [WINDOWPROP FMENU 'MINSIZE (create POSITION                                            XCOORD _ (fetch (REGION WIDTH) of (WINDOWPROP                                                                               FMENU                                                                               'REGION))                                            YCOORD _ (fetch (REGION HEIGHT)                                                        of (WINDOWPROP FMENU 'REGION]          (WINDOWPROP FMENU 'FM.PROMPTWINDOW (GETPROMPTWINDOW MAINWINDOW))          (ATTACHWINDOW FMENU MAINWINDOW 'TOP 'JUSTIFY 'LOCALCLOSE)          (WINDOWADDPROP FMENU 'REJECTMAINCOMS 'SHAPEW)          (WINDOWADDPROP MAINWINDOW 'RESHAPEFN 'REPOSITIONATTACHEDWINDOWS)          (RS232CMENU.FILLIN.PARAMETERS FMENU DEVICENAME])(RS232CMENU.CHANGEDFN  [LAMBDA (FREEMENU WINDOW BUTTON)                           (* ; "Edited  5-Nov-87 11:25 by FS")    (CLEARW (GETPROMPTWINDOW (MAINWINDOW WINDOW)                   NIL NIL T]))(RPAQQ DLRS232C.FMENU.SPEC       [(PROPS FONT (MODERN 10 BOLD))        ((LABEL "Apply!" MESSAGE "Makes the changes you've selected" SELECTEDFN RS232C.FMENU.APPLY)         (LABEL "Abort!" MESSAGE "Closes this menu" SELECTEDFN RS232C.FMENU.ABORT)         (LABEL "Show!" MESSAGE "Updates this menu to show current RS232C settings" SELECTEDFN                 RS232C.FMENU.SHOW)         (LABEL "SendBreak!" MESSAGE "Sends a short break" SELECTEDFN RS232C.FMENU.SENDBREAK)         (LABEL "SendLongBreak!" MESSAGE "Sends a long break" SELECTEDFN RS232C.FMENU.SENDLONGBREAK)         (LABEL "Hangup!" MESSAGE "Tries to hangup a modem connection" SELECTEDFN RS232C.FMENU.HANGUP                ))        ((GROUP (PROPS FORMAT TABLE)                ((TYPE STATE LABEL "Baud Rate:" ID BaudRate MENUITEMS                       (50 75 110 134.5 150 300 600 1200 2400 3600 4800 7200 9600 19200 28800 38400                            48000 56000 57600)                       MENUFONT                       (MODERN 10)                       MENUTITLE "Available baud rates" SELECTEDFN RS232CMENU.CHANGEDFN LINKS                       (DISPLAY BAUDRATEDISPLAY))                 (TYPE DISPLAY LABEL 57600 ID BAUDRATEDISPLAY FONT (MODERN 10))                 (TYPE STATE LABEL "Parity:" ID Parity MENUITEMS (None Even Odd)                       MENUFONT                       (MODERN 10)                       MENUTITLE "Available Parity Settings" LINKS (DISPLAY PARITYDISPLAY)                       SELECTEDFN RS232CMENU.CHANGEDFN)                 (TYPE DISPLAY LABEL None ID PARITYDISPLAY FONT (MODERN 10))                 (TYPE STATE LABEL "Character Length:" ID CharLength MENUITEMS (Five Six Seven Eight)    