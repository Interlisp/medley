         (IRIS.ECHOFF sppStream)
         (IRIS.GCMD 77 sppStream)
         (IRIS.FLUSHG sppStream)
         (SETQ retval (IRIS.RECO sppStream))
         (IRIS.RECCR sppStream)
         (IRIS.ECHOON sppStream)
         retval])

