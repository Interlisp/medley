      else (SETQ sppStream IRISCONN))
    (LET (retval)
         (IRIS.ECHOFF sppStream)
         (IRIS.GCMD 316 sppStream)
         (IRIS.FLUSHG sppStream)
         (SETQ retval (IRIS.RECL sppStream))
         (IRIS.RECCR sppStream)
         (IRIS.ECHOON sppStream)
         retval])

