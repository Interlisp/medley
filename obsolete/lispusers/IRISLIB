             (SET wtm (IRIS.RECS sppStream))
             (SET b (IRIS.RECO sppStream))
             (IRIS.RECCR sppStream)
             (IRIS.ECHOON sppStream)
      else (IRIS\ERROR %'IRIS.GETCURSOR %' (index color wtm b])

