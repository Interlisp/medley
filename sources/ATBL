        (replace CONTROLFLG of TTBL with (AND MODE T)))])

(COPYTERMTABLE
  [LAMBDA (TTBL)                                         (* lmm "14-APR-81 14:27")
    (create TERMTABLEP using (SETQ TTBL (\GTTERMTABLE TTBL T))
                                 TERMSA _ (create CHARTABLE using (fetch TERMSA
