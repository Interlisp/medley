       (NEWVALUEP (replace (READTABLEP NUMBERBASE) of RDTBL                                                       with NEWVALUE])                   (NAME [LET ((OLDNAME (fetch (READTABLEP READTBLNAME) of RDTBL)))                              (PROG1 OLDNAME (COND                                                (NEWVALUEP (COND                                                              (OLDNAME (REMHASH OLDNAME                                                                               \READTABLEHASH)))                                                       (replace (READTABLEP READTBLNAME) of RDTBL                                                          with NEWVALUE)                                                       (PUTHASH NEWVALUE RDTBL \READTABLEHASH])                   (COMMONLISP [PROG1 (fetch (READTABLEP COMMONLISP) of RDTBL)                                      (COND                                         (NEWVALUEP (replace (READTABLEP COMMONLISP) of RDTBL                                                       with NEWVALUE)                                                (if NEWVALUE                                                    then     (* ;                                            "COMMONLISP implies COMMONNUMSYNTAX and not USESILPACKAGE")                                                         (replace (READTABLEP COMMONNUMSYNTAX)                                                            of RDTBL with T)                                                         (replace (READTABLEP USESILPACKAGE)                                                            of RDTBL with NIL])                   (COMMONNUMSYNTAX                         [PROG1 (fetch (READTABLEP COMMONNUMSYNTAX) of RDTBL)                               (COND                                  (NEWVALUEP (replace (READTABLEP