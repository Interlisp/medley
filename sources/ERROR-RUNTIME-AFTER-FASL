NS:INVOKE-RESTART CONDITIONS::R))))(CL:DEFUN CONDITIONS:MUFFLE-WARNING ()   (LET ((CONDITIONS::R (CONDITIONS:FIND-RESTART 'CONDITIONS:MUFFLE-WARNING)))        (CL:WHEN CONDITIONS::R (CONDITIONS:INVOKE-RESTART CONDITIONS::R))))(DEFINE-PROCEED-FUNCTION USE-VALUE :REPORT "Use a different value"   XCL::VALUE)(DEFINE-PROCEED-FUNCTION STORE-VALUE :REPORT "Store a new value and use it"   XCL::VALUE)(PUTPROPS ERROR-RUNTIME-AFTER-FASL COPYRIGHT ("Venue & Xerox Corporation" 1987 1988 1990))(DECLARE\: DONTCOPY  (FILEMAP (NIL)))STOP