   do (for S2 on (CDR S1) do (PRIN2 NAME T T)                                                                  (AND (CAAR S1)                                                                       (PRIN1 " from " T)                                                                       (PRIN2 (CAAR S1)                                                                              T T))                                                                  (PRIN1 " and " T)                                                                  (PRIN2 NAME T T)                                                                  (COND                                                                     ((CAAR S2)                                                                      (PRIN1 " from " T)                                                                      (PRIN2 (CAAR S2)                                                                             T T)))                                                                  (PRIN1 " differ:" T)                                                                  (TERPRI T)                                                                  (COMPARELISTS (CDAR S1)                                                                         (CDAR S2]                      'DIFFERENT)                     (T 'SAME])(COMPARE  [LAMBDA (NAME1 NAME2 TYPE SOURCE1 SOURCE2)             (* lmm " 5-SEP-78 13:37")    (PROG [[DEF1 (GETDEF NAME1 TYPE SOURCE1 '(NOERROR NOCOPY]           (DEF2 (GETDEF NAME2 TYPE SOURCE2 '(NOERROR NOCOPY]          (COND             ((COMPARELST DEF1 DEF2)              (RETURN)))          (PRIN2 NAME1 T T)          (COND             (SOURCE1 (PRIN1 " from " T)                    (PRIN2 SOURCE1 T T)))          (PRIN1 " and " T)          (PRIN2 NAME2 T T)          (COND             (SOURCE2 (PRIN1 " from " T)                    (PRIN2 SOURCE2 T T)))          (PRIN1