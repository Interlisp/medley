                (FONTAVGCHARWIDTH WORD)        (* ;                           "Set in FONTCREATE, used to fix up the linelength when DSPFONT is called")                              (FONTIMAGEWIDTHS POINTER)      (* ; "This is the image width, as opposed to the advanced width;  initial hack for accents, kerning.  Fields is referenced by FONTCREATE.")                              (FONTCHARSETVECTOR POINTER)    (* ; "A 256-pointer block, with one pointer per 'character set' --each group of 256 character codes.  Each pointer is either NIL if there's no info for that charset, or is a CHARSETINFO, containing widths, char bitmap, etc for the characters in that charset.")                              (FONTEXTRAFIELD2 POINTER))                             FONTCHARSETVECTOR _ (\CREATEFONTCHARSETVECTOR))(RECORD FONTFACE (WEIGHT SLOPE EXPANSION)                     [ACCESSFNS ((COLOR (CDDDR DATUM)                                        (RPLACD (CDDR DATUM)                                               NEWVALUE))                                 (BACKCOLOR [COND                                               ((CDDDR DATUM)                                                (CAR (CDDDR DATUM]                                        (PROGN [COND                                                  ((NULL (CDDDR DATUM))                                                   (RPLACD (CDDR DATUM)                                                          (LIST NIL NIL]                                               (RPLACA (CDDDR DATUM)                                                      NEWVALUE)))                                 (FORECOLOR [COND                                               ((CDDDR DATUM)                                                (CADR (CDDDR DATUM]                                        (PROGN [COND                                                  ((NULL (CDDDR DATUM))                                                   (RPLACD (CDDR