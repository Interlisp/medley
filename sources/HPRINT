                  HPRPTSTRING)
              (SETQ RPTCNT (READ FILE RDTBL))
              (SETQ RPTVAL (READ FILE RDTBL))
              (HVREADEND FILE RDTBL)
              (GO LOOP))
             (T (RETURN RPTVAL])

