(HPRINT-TO-STRING
  [LAMBDA (VALUE NO-CIRCLE-FLAG)                         (* ; "Edited  9-Sep-87 16:21 by amd")
    (CL:WITH-OUTPUT-TO-STRING (S)
           (HPRINT VALUE S NO-CIRCLE-FLAG])

(HPRINT-TO-STRINGS
  [LAMBDA (VALUE NO-CIRCLE-FLAG)                         (* ; "Edited  5-Feb-88 14:42 by amd")
    (XCL:WITH-COLLECTION
     (XCL:COLLECT (CL:WITH-OUTPUT-TO-STRING
                   (S)
                   (HANDLER-BIND [(END-OF-FILE #'(CL:LAMBDA (C)
                                                        (CL:WHEN (AND (EQ (END-OF-FILE-STREAM C)
                                                                          S)
                                                                      (CONDITIONS:FIND-RESTART
                                                                       'SI::RETRY-OUTCHAR))
                                                            (XCL:COLLECT (CL:GET-OUTPUT-STREAM-STRING
                                                                          S))
                                                            (CONDITIONS:INVOKE-RESTART 
                                                                   'SI::RETRY-OUTCHAR))]
                          (HPRINT VALUE S NO-CIRCLE-FLAG])
)
