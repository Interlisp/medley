(\EVENTKEYS  [LAMBDA NIL                                            (* rmk%: " 4-JUN-81 22:58")          (* Returns the state of the various keys that are represented in mouse events)    (LOGOR (COND              ((KEYDOWNP 'LOCK)               128)              (T 0))           (COND              ((KEYDOWNP 'LSHIFT)               64)              (T 0))           (COND              ((KEYDOWNP 'CTRL)               32)              (T 0))           (COND              ((KEYDOWNP 'RSHIFT)               8)              (T 0))           (COND              ((KEYDOWNP 'BLANK-TOP)               4)              (T 0))           (COND              ((KEYDOWNP 'BLANK-MIDDLE)               2)              (T 0))           (COND              ((KEYDOWNP 'BLANK-BOTTOM)               1)              (T 0])
)
