(DEFINEQ

(WITHOUT-INTERRUPTS  [NLAMBDA (FORM)                                        (* lmm "18-Apr-85 02:53")    (PROG (VAL)          (\KEYBOARDOFF)          (SETQ VAL (DISPLAYDOWN FORM))          (\KEYBOARDON)          (RETURN VAL])
)



